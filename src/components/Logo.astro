                <path fill-rule="evenodd" clip-rule="evenodd" d="M0 200V0H400V200V400H0V200ZM300 100C355.228 100 400 144.772 400 200H200C200 144.772 244.772 100 300 100ZM0 200C0 255.228 44.7715 300 100 300C155.228 300 200 255.228 200 200H0Z" fill="black"/>
            </svg>
            <svg width="400" height="400" viewBox="0 0 400 400" fill="none" xmlns="http://www.w3.org/2000/svg">
                <path d="M100 300C44.7715 300 0 344.772 0 400V200H200C200 144.772 155.229 100 100 100C44.7715 100 0 144.772 0 200V0H400V400H200C200 344.772 155.229 300 100 300Z" fill="black"/>
            </svg>
            <svg width="400" height="400" viewBox="0 0 400 400" fill="none" xmlns="http://www.w3.org/2000/svg">
                <path d="M100 300C44.7715 300 0 344.772 0 400V0H400V200C400 144.772 355.229 100 300 100C244.771 100 200 144.772 200 200H400V400H200C200 344.772 155.229 300 100 300Z" fill="black"/>
